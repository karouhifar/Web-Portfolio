<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>About</title>
  <!-- ico in the URL address bar and on bookmarks created by web browsers -->
  <link rel="icon" type="image/png" href="img/Kamyab.jpg" />
  <!-- Bootstrap CSS file -->
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">

  <!-- fontAwesome file -->
  <script src="https://kit.fontawesome.com/b0db9e868b.js" crossorigin="anonymous"></script>
  <!-- Custom CSS file -->
  <link rel="stylesheet" href="css/style-new.css" />
  <link rel="stylesheet" href="css/about.css" />
  <!-- GSAP animation -->
  <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.6.1/gsap.min.js"></script>
  {{!-- Animation JS Library --}}
  <script src="https://cdn.jsdelivr.net/npm/animejs@3.0.1/lib/anime.min.js"></script>
</head>

<body>
  <!-- START HEADER -->
  {{> header headerDATA}}
  <!-- END HEADER -->
  <!-- START MAIN AREA -->
  <main>
    {{!-- Start about us --}}
    <section class="about-us">
      <div class="container">
        <h1 class="title text-center text-uppercase">About us</h1>
        <div class="row">
          <div class="col-lg-6 col-md-12 about-us-detail">
            <p class="about-title">Why we're different</p>
            <ul class="list-group list-group-flush">
              <li class="list-group-item"> We demonstrate strong communication skills for our clients.</li>
              <li class="list-group-item">We determine coding requirements for site creation, including security and
                specialized scripts</li>
              <li class="list-group-item">We design and develop simple and user-friendly websites.</li>
              <li class="list-group-item">We improve web functionality and speed Project management task to meet
                deadlines.</li>
              <li class="list-group-item">We provide the best customer service satisfaction for our clients.</li>
            </ul>
          </div>
          <div class="col-lg-6 col-md-12 about-us-detail">
            <img src="img/aboutImg.png" alt="aboutImg" class="img-fluid">
          </div>

        </div>
      </div>
    </section>
    {{!-- End about us --}}

    {{!-- Start Testimonials --}}
    <section class="testimonials mb-5 gs_reveal" id="scrollPosition">
      <div class="container">
        <h1 class="title text-center text-uppercase">What others say</h1>
        <div class="row testimonials offset-2">
          <div class="col-lg-5 col-md-12 testimonials-details gs_reveal">
            <p>Lorem ipsum dolor sit amet consectetur adipisicing elit.
              Porro voluptatibus minima obcaecati. Consequatur hic suscipit dolor laudantium minima alias soluta?</p>
            <div class="d-flex flex-row">
              <img src="img/testimonials/t1.jpg" alt="Jack">
              <p class="user-details align-self-end"><b>Jack</b><br><small class="blockquote-footer">Co-founder</small>
              </p>
            </div>
          </div>
          <div class="col-lg-5 col-md-12 testimonials-details gs_reveal">
            <p>Lorem ipsum dolor sit amet consectetur adipisicing elit.
              Porro voluptatibus minima obcaecati. Consequatur hic suscipit dolor laudantium minima alias soluta?</p>
            <div class="d-flex flex-row">
              <img src="img/testimonials/t2.jpg" alt="Daniel">
              <p class="user-details align-self-end"><b>Daniel</b><br><small class="blockquote-footer">Manager</small>
              </p>
            </div>
          </div>
        </div>
      </div>
    </section>

    {{!-- END Testimonials --}}
  </main>
  <!-- END MAIN AREA -->

  <!-- START FOOTER AREA -->
  {{>footer}}
  <!-- END FOOTER AREA -->
  <!-- Bootstrap JS file -->
  <script src="js/jquery-3.6.0.min.js"></script>

  <script>
    const tl = gsap.timeline({ defaults: { duration: 1, stagger: 0.7 } });
    tl.from(".headerMain", { opacity: 0.1, x: -500 })
      .from(".about-us", { opacity: 0.1, y: 500 }, '-=0.5')
      .from(".about-us-detail", { opacity: 0.1, x: -500 }, '-=1')
  </script>
  <script src="js/preLoad.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
  {{!-- ScrollTrigger GSAP --}}
  <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.6.1/ScrollTrigger.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/2.0.1/TweenMax.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/2.1.3/TimelineMax.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/ScrollMagic/2.0.5/ScrollMagic.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/ScrollMagic/2.0.5/plugins/debug.addIndicators.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/3.8.0/CSSRulePlugin.min.js"
    integrity="sha512-1z2hYE5HWzZW8cAJRhlwT472AGZC3HlJyWYmW3SXG/Qh0S6TFdkYEqJaPwT7eCy9tI4KvwrRVK6IGqtqErpHQg=="
    crossorigin="anonymous" referrerpolicy="no-referrer"></script>
  <!-- Custom JS file-->
  <script src="js/scrollHeader.js"></script>

</body>

</html>